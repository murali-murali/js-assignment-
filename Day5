        <script> 
          class Persons { 
constructor(name, age,email) { 

this.name = name; 
this.age = age; 
this.email = email; 
}

login() { 
console.log(`${this.name} has logged in`); 
return this; 
} 

logout() {
console.log(`${this.name} has logged out`); 
return this;
}
}


class Moderator extends Persons { 

constructor(name,age,email) { 

super(name,age,email);
 this.addCoins=0;

  this.removeCoins;

}

addCoins(Persons) {
 this.Coins++; 
console.log(`${this.name} has ${this.Coins} coins`); 
return this;
}

removeCoins(Persons) {
this.coins--;
console.log(`${this.name} has ${this.Coins} coins` );
return this;

}
}

